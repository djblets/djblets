{
    "env": {
        "development": {
            "presets": [
                "@babel/preset-env",
                [
                    "@babel/preset-typescript",
                    {
                        "allowDeclareFields": true
                    }
                ]
            ]
        }
    },
    "plugins": [
        "babel-plugin-dedent",
        "babel-plugin-django-gettext",
        ["@babel/plugin-proposal-decorators", {
            "version": "legacy"
        }]
    ],
    "sourceMaps": true
}
